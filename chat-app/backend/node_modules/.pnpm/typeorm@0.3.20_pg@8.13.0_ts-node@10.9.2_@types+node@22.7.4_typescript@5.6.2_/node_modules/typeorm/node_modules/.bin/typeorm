#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@22.7.4_typescript@5.6.2_/node_modules/typeorm/node_modules:/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@22.7.4_typescript@5.6.2_/node_modules:/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@22.7.4_typescript@5.6.2_/node_modules/typeorm/node_modules:/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@22.7.4_typescript@5.6.2_/node_modules:/home/nirjla/Documents/amnil-intern/chat-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
